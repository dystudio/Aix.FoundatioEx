<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>Aix.FoundatioEx.Kafka</RootNamespace>
    <AssemblyName>Aix.FoundatioEx.Kafka</AssemblyName>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>Aix.FoundatioEx.Kafka</PackageId>
    <Authors>linzhiqiang</Authors>
    <Version>1.0.2</Version>
    <Description>Foundatio的MessageBus的kafka实现
支持手工提交offset模式
支持单topic和多topic模式(所有类型消息存储在单个topic或者多个topic)</Description>
    <PackageProjectUrl>https://github.com/linzhiqiang/Aix.FoundatioEx.git</PackageProjectUrl>
    <RepositoryUrl>https://github.com/linzhiqiang/Aix.FoundatioEx.git</RepositoryUrl>
    <PackageReleaseNotes>消费者增加ManualCommitBatch参数，手工提交时 消费多少条消息提交一次，提高消费效率</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Confluent.Kafka" Version="1.0.0" />
    <PackageReference Include="Foundatio" Version="8.0.1965" />
  </ItemGroup>

</Project>
